@Article{Luikart2010,
  Title                    = {Estimation of census and effective population sizes: the increasing usefulness of DNA-based approaches},
  Author                   = {Luikart, G. and Ryman, N. and Tallmon, D. A. and Schwartz, M. K. and Allendorf, F. W.},
