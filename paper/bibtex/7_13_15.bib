@Article{Barb2014,
  Title                    = {Chromosomal evolution and patterns of introgression in helianthus},
  Author                   = {Barb, J. G. and Bowers, J. E. and Renaut, S. and Rey, J. I. and Knapp, S. J. and Rieseberg, L. H. and Burke, J. M.},
