@Article{Catchen2013,
  Title                    = {Stacks: an analysis tool set for population genomics},
  Author                   = {Catchen, J. and Hohenlohe, P. A. and Bassham, S. and Amores, A. and Cresko, W. A.},
  Journal                  = {Mol Ecol},
