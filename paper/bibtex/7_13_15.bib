@Article{Seeb2011a,
  Title                    = {Transcriptome sequencing and high-resolution melt analysis advance single nucleotide polymorphism discovery in duplicated salmonids},
  Author                   = {Seeb, J. E. and Pascal, C. E. and Grau, E. D. and Seeb, L. W. and Templin, W. D. and Harkins, T. and Roberts, S. B.},
