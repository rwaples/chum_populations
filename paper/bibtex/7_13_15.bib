@Article{Kozfkay2008,
  Title                    = {A genetic evaluation of relatedness for broodstock management of captive, endangered Snake River sockeye salmon, Oncorhynchus nerka},
  Author                   = {Kozfkay, C. C. and Campbell, M. R. and Heindel, J. A. and Baker, D. J. and Kline, P. and Powell, M. S. and Flagg, T.},
  Journal                  = {Conservation Genetics},
