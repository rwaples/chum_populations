@Article{Palti2014,
  Title                    = {A resource of single-nucleotide polymorphisms for rainbow trout generated by restriction-site associated DNA sequencing of doubled haploids},
  Author                   = {Palti, Y. and Gao, G. and Miller, M. R. and Vallejo, R. L. and Wheeler, P. A. and Quillet, E. and Yao, J. and Thorgaard, G. H. and Salem, M. and Rexroad, C. E., 3rd},
