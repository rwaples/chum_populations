@Article{Corbett-Detig2013,
  Title                    = {Genetic incompatibilities are widespread within species},
  Author                   = {Corbett-Detig, R. B. and Zhou, J. and Clark, A. G. and Hartl, D. L. and Ayroles, J. F.},
  Journal                  = {Nature},
