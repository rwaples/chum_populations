@Article{Rastas2013,
  Title                    = {Lep-MAP: fast and accurate linkage map construction for large SNP datasets},
  Author                   = {Rastas, P. and Paulin, L. and Hanski, I. and Lehtonen, R. and Auvinen, P.},
