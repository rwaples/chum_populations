@Article{Tracy1994,
  Title                    = {Level Spacing Distributions and the Bessel Kernel},
  Author                   = {Tracy, C. A. and Widom, H.},
  Journal                  = {Communications in Mathematical Physics},
  Year                     = {1994},
  Number                   = {2},
