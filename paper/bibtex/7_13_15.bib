@Article{Greene1997,
  Title                    = {SINE and transposon sequences generate high-resolution DNA fingerprints, ''SINE prints,'' that exhibit faithful Mendelian inheritance in pink salmon (Oncorhynchus gorbuscha)},
  Author                   = {Greene, B. A. and Seeb, J. E.},
  Journal                  = {Molecular Marine Biology and Biotechnology},
  Year                     = {1997},
  Number                   = {4},
  Pages                    = {328-338},
  Volume                   = {6},

  Abstract                 = {We report the development of a polymerase chain reaction (PCR)-based technique to obtain high-resolution fluorescent DNA fingerprints from individual pink salmon (Oncorhynchus gorbuscha). A dye-labeled primer complementary to the Tcl-like transposon and an unlabeled primer for the Sma1 SINE repeat motif were used to simultaneously amplify between so and 80 fragments ranging in size from 75 to 550 bases using PCR. These fluorescently labeled DNAs were separated on a denaturing acrylamide gel, resolved using an automated DNA sequencing instrument, and analyzed using Genescan and Genotyper software. We found the complex pattern of amplified fragments to be a reliable, specific fingerprint for the individual. Indeed, ''SINE-prints'' have sufficient resolution to discriminate among progeny from different families of single-pair matings of hatchery broodstock. Here we show faithful Mendelian inheritance of these fragments within families of single-pair matings, determine gene-centromere map distances for selected amplified fragments in four families of gynogenetic diploid siblings, and test for linkage among amplified fragments segregating in diploid families, Linkage was found between a small number of fragment pairs, and 37% of the fragments were tightly linked to the centromere. The majority of fragments, however, segregated independently and appear to be dispersed throughout the genome. This DNA fingerprinting technology promises to be a powerful tool with many population and molecular genetic applications for salmonids and other species.},
