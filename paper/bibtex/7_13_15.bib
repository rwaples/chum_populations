@Article{Rexroad2008,
  Title                    = {A second generation genetic map for rainbow trout (Oncorhynchus mykiss)},
  Author                   = {Rexroad, C. E., 3rd and Palti, Y. and Gahr, S. A. and Vallejo, R. L.},
