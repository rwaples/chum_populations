@Article{Hohenlohe2013,
  Title                    = {Genomic patterns of introgression in rainbow and westslope cutthroat trout illuminated by overlapping paired-end RAD sequencing},
  Author                   = {Hohenlohe, P. A. and Day, M. D. and Amish, S. J. and Miller, M. R. and Kamps-Hughes, N. and Boyer, M. C. and Muhlfeld, C. C. and Allendorf, F. W. and Johnson, E. A. and Luikart, G.},
