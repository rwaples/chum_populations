@Article{Pfender2011,
  Title                    = {Mapping with RAD (restriction-site associated DNA) markers to rapidly identify QTL for stem rust resistance in Lolium perenne},
  Author                   = {Pfender, W. F. and Saha, M. C. and Johnson, E. A. and Slabaugh, M. B.},
