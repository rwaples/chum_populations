@Article{Kuttner2011,
  Title                    = {Genetic architecture of body weight, condition factor and age of sexual maturation in Icelandic Arctic charr (Salvelinus alpinus)},
  Author                   = {Kuttner, E. and Moghadam, H. K. and Skulason, S. and Danzmann, R. G. and Ferguson, M. M.},
