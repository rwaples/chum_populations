@Article{Andersen2012,
  Title                    = {Chromosome-scale selective sweeps shape Caenorhabditis elegans genomic diversity},
  Author                   = {Andersen, E. C. and Gerke, J. P. and Shapiro, J. A. and Crissman, J. R. and Ghosh, R. and Bloom, J. S. and Felix, M. A. and Kruglyak, L.},
  Journal                  = {Nat Genet},
  Year                     = {2012},
  Number                   = {3},
