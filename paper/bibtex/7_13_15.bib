@Article{Langham2004,
  Title                    = {Genomic duplication, fractionation and the origin of regulatory novelty},
  Author                   = {Langham, R. J. and Walsh, J. and Dunn, M. and Ko, C. and Goff, S. A. and Freeling, M.},
