@Article{Seeb2014a,
  Title                    = {Parallel signatures of selection in temporally isolated lineages of pink salmon},
  Author                   = {Seeb, L. W. and Waples, R. K. and Limborg, M. T. and Warheit, K. I. and Pascal, C. E. and Seeb, J. E.},
  Journal                  = {Mol Ecol},
