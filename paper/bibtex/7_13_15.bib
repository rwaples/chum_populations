  Author                   = {Spruell, P. and Pilgrim, K. L. and Greene, B. A. and Habicht, C. and Knudsen, K. L. and Lindner, K. R. and Olsen, J. B. and Sage, G. K. and Seeb, J. E. and Allendorf, F. W.},
  Journal                  = {J Hered},
