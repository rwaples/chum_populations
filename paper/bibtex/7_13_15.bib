@Article{Nosil2012a,
  Title                    = {Genomic consequences of multiple speciation processes in a stick insect},
  Author                   = {Nosil, P. and Gompert, Z. and Farkas, T. E. and Comeault, A. A. and Feder, J. L. and Buerkle, C. A. and Parchman, T. L.},
  Journal                  = {Proc Biol Sci},
