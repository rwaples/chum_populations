@Article{Phillips2009,
  Title                    = {Recombination is suppressed over a large region of the rainbow trout Y chromosome},
  Author                   = {Phillips, R. B. and DeKoning, J. J. and Ventura, A. B. and Nichols, K. M. and Drew, R. E. and Chaves, L. D. and Reed, K. M. and Felip, A. and Thorgaard, G. H.},
