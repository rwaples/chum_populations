@Article{Hess2013,
  Title                    = {Population genomics of Pacific lamprey: adaptive variation in a highly dispersive species},
  Author                   = {Hess, J. E. and Campbell, N. R. and Close, D. A. and Docker, M. F. and Narum, S. R.},
  Journal                  = {Mol Ecol},
