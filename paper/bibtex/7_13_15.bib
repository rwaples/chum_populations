@Article{Quinn2011,
  Title                    = {Identification of genes associated with heat tolerance in Arctic charr exposed to acute thermal stress},
  Author                   = {Quinn, N. L. and McGowan, C. R. and Cooper, G. A. and Koop, B. F. and Davidson, W. S.},
