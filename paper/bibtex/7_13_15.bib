@Article{Amores2011,
  Title                    = {Genome evolution and meiotic maps by massively parallel DNA sequencing: spotted gar, an outgroup for the teleost genome duplication},
  Author                   = {Amores, A. and Catchen, J. and Ferrara, A. and Fontenot, Q. and Postlethwait, J. H.},
