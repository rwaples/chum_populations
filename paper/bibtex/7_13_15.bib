@Article{Cooper2008,
  Title                    = {Systematic assessment of copy number variant detection via genome-wide SNP genotyping},
  Author                   = {Cooper, G. M. and Zerr, T. and Kidd, J. M. and Eichler, E. E. and Nickerson, D. A.},
